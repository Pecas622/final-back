<h1>Listado de usuarios</h1>
<div>
    <ul>
        {{#each users}}
            <li>
                <strong>Nombre: </strong>{{this.first_name}}
                <strong>Apellido: </strong>{{this.last_name}}
                <strong>Email: </strong>{{this.email}}
                <strong>Genero: </strong>{{this.genero}}
            </li>
            <hr>
        {{/each}}
    </ul>
</div>
<div>
        <center>
            {{#if hasPrevPage }}
                <a href="/users?numPage={{prevPage}}">Anterior</a>
            {{/if }}
            <label>{{page}}</label>
            {{#if hasNextPage }}
                <a href="/users?numPage={{nextPage}}">Siguiente</a>
            {{/if }}
        </center>
</div>